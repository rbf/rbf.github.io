<script id="x-rbfe-script">window.xrbfe='{{site.email_user}}YYYYY'+document.domain.replace(/\./,'XXXXX');document.styleSheets[0].insertRule('x-rbfe{cursor:pointer;}', 1);document.styleSheets[0].insertRule('x-rbfe:after{content:"' + window.xrbfe.replace(/YYYYY/,'\u0040').replace(/XXXXX/,'\u002E') + '";}', 1);document.getElementsByTagName('head')[0].removeChild(document.getElementById("x-rbfe-script"));</script>